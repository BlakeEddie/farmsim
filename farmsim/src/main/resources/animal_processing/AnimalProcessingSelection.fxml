<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="mainPane" maxHeight="620.0" maxWidth="620.0" prefHeight="620.0" prefWidth="620.0"
            xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="farmsim.ui.AnimalProcessingSelectionController">
    <center>
        <BorderPane fx:id="contentBorderPane" maxHeight="600.0" maxWidth="600.0" prefHeight="600.0" prefWidth="600.0"
